{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "components/*": [
        "./components/*"
      ],
      "screens/*": [
        "./app/*"
      ],
      "assets/*": [
        "./assets/*"
      ],
      "hooks/*": [
        "./hooks/*"
      ],
      "constants/*": [
        "./constants/*"
      ],
      "react": [
        "./node_modules/@types/react"
      ],
      "react-native": [
        "./node_modules/@types/react-native"
      ]
    }
  },
  "include": [
    "app",
    "components",
    "assets",
    "hooks",
    "constants",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
