{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "prepare": {
      "inputs": [
        "package.json"
      ]
    },
    "build:web": {
      "cache": true,
      "persistent": true,
      "inputs": [
        "yarn.lock",
        "**/package.json",
        "src/**.ts",
        "src/**.tsx"
      ],
      "outputs": [
        "./apps/web/build"
      ],
      "dependsOn": [
        "^build"
      ],
      "env": [
        "REACT_APP_WEB3AUTH_CLIENT_ID",
        "REACT_APP_SOCKET_BRIDGE_KEY",
        "REACT_APP_RAMP_TRANSAK_API_KEY",
        "REACT_APP_TENDERLY_ACCESS_TOKEN",
        "REACT_APP_TENDERLY_PROJECT_SLUG",
        "REACT_APP_TENDERLY_USER_NAME",
        "REACT_APP_WERT_PARTNER_ID",
        "REACT_APP_SOCKET_API_KEY",
        "REACT_APP_INFURA",
        "REACT_APP_GATEFI_MERCHANT_ID",
        "REACT_APP_FRONT_URL",
        "REACT_APP_FRONT_API_KEY",
        "REACT_APP_FRONT_CLIENT_ID",
        "REACT_APP_FRONT_URL_SANDBOX",
        "REACT_APP_FRONT_API_KEY_SANDBOX",
        "REACT_APP_IS_LEGACY"
      ]
    },
    "start:web": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "prepare"
      ],
      "inputs": [
        "apps/web/src/**",
        "apps/web/index.html",
        "apps/web/public/**",
        "apps/web/package.json",
        "apps/web/vite.config.ts"
      ]

    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "check-types": {
      "dependsOn": ["^check-types"],
      "outputs": []
    }
  }
}